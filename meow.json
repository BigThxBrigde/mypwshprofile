{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": "ðŸ˜ºðŸ’¬ <#FF8000>Meow!?</> {{ .Path }} ",
          "foreground": "#56B6C2",
          "type": "path"
        },
        {
          "properties": {
            "branch_icon": "",
            "fetch_status": true
          },
          "style": "plain",
          "template": "<#5FAAE8>git:(</>{{ .HEAD }} {{ .BranchStatus }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0}} \udb80\udd93 {{ .StashCount }}{{ end }}{{ if gt .WorktreeCount 0}} \uf1bb {{ .WorktreeCount }}{{ end }}<#5FAAE8>)</>",
          "foreground": "#D0666F",
          "type": "git"
        },
        {
          "properties": {
            "time_format": "15:04"
          },
          "style": "plain",
          "template": "{{ .CurrentDate | date .Format }} ",
          "foreground": "#66F68F",
          "type": "time"
        },
        {
          "style": "powerline",
          "template": " \ue77f {{ .Full }} ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "background": "#00ffff",
          "type": "dotnet"
        },
        {
          "style": "powerline",
          "template": " \ue718 {{ .Full }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#6CA35E",
          "type": "node"
        },
        {
          "style": "powerline",
          "template": " \ue7a8 {{ .Full }} ",
          "foreground": "#193549",
          "powerline_symbol": "\ue0b0",
          "background": "#99908a",
          "type": "rust"
        },
        {
          "style": "powerline",
          "template": " \ue235 {{ .Full }} ",
          "foreground": "#100e23",
          "powerline_symbol": "\ue0b0",
          "background": "#906cff",
          "type": "python"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "style": "powerline",
          "template": "âžœ ",
          "foreground": "lightGreen",
          "type": "text"
        },
        {
          "style": "plain",
          "template": " \uf0e7 ",
          "foreground": "#26C6DA",
          "type": "root"
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
